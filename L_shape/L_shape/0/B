FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      B;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -2 0 0 -1 0];

internalField   uniform (0 0 0.28);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            fixedValue;
        value           $internalField;
    }

    Wall_side
    {
        type            fixedValue;
        value           $internalField;
    }


    Wall_out
    {
        type            fixedValue;
        value           $internalField;
    }
	Wall_inside
    {
        type            fixedValue;
        value           $internalField;
    }



}


